#! /bin/bash

## This shell script will be used to execute each of the unit tests
## and record the results in a log file

## create log file
now=$(date +"%d.%m.%y.%T")
touch $now.log

## ArrayApp - ordered tests
java inventory.ArrayApp -f data.csv -t 1000 1000  >> $now.log
java inventory.ArrayApp -f data.csv -t 2000 2000  >> $now.log
java inventory.ArrayApp -f data.csv -t 3000 3000  >> $now.log
java inventory.ArrayApp -f data.csv -t 4000 4000  >> $now.log
java inventory.ArrayApp -f data.csv -t 5000 5000  >> $now.log
java inventory.ArrayApp -f data.csv -t 6000 6000  >> $now.log
java inventory.ArrayApp -f data.csv -t 7000 7000  >> $now.log
java inventory.ArrayApp -f data.csv -t 8000 8000  >> $now.log
java inventory.ArrayApp -f data.csv -t 9000 9000  >> $now.log
java inventory.ArrayApp -f data.csv -t 10000 10000  >> $now.log

## ArrayApp - random tests
java inventory.ArrayApp -f rand-data.csv -t 1000 1000  >> $now.log
java inventory.ArrayApp -f rand-data.csv -t 2000 2000  >> $now.log
java inventory.ArrayApp -f rand-data.csv -t 3000 3000  >> $now.log
java inventory.ArrayApp -f rand-data.csv -t 4000 4000  >> $now.log
java inventory.ArrayApp -f rand-data.csv -t 5000 5000  >> $now.log
java inventory.ArrayApp -f rand-data.csv -t 6000 6000  >> $now.log
java inventory.ArrayApp -f rand-data.csv -t 7000 7000  >> $now.log
java inventory.ArrayApp -f rand-data.csv -t 8000 8000  >> $now.log
java inventory.ArrayApp -f rand-data.csv -t 9000 9000  >> $now.log
java inventory.ArrayApp -f rand-data.csv -t 10000 10000  >> $now.log

## HashApp- ordered tests
java inventory.HashApp -f data.csv -t 1000 1000  >> $now.log
java inventory.HashApp -f data.csv -t 2000 2000  >> $now.log
java inventory.HashApp -f data.csv -t 3000 3000  >> $now.log
java inventory.HashApp -f data.csv -t 4000 4000  >> $now.log
java inventory.HashApp -f data.csv -t 5000 5000  >> $now.log
java inventory.HashApp -f data.csv -t 6000 6000  >> $now.log
java inventory.HashApp -f data.csv -t 7000 7000  >> $now.log
java inventory.HashApp -f data.csv -t 8000 8000  >> $now.log
java inventory.HashApp -f data.csv -t 9000 9000  >> $now.log
java inventory.HashApp -f data.csv -t 10000 10000  >> $now.log

## HashApp - random tests
java inventory.HashApp -f rand-data.csv -t 1000 1000  >> $now.log
java inventory.HashApp -f rand-data.csv -t 2000 2000  >> $now.log
java inventory.HashApp -f rand-data.csv -t 3000 3000  >> $now.log
java inventory.HashApp -f rand-data.csv -t 4000 4000  >> $now.log
java inventory.HashApp -f rand-data.csv -t 5000 5000  >> $now.log
java inventory.HashApp -f rand-data.csv -t 6000 6000  >> $now.log
java inventory.HashApp -f rand-data.csv -t 7000 7000  >> $now.log
java inventory.HashApp -f rand-data.csv -t 8000 8000  >> $now.log
java inventory.HashApp -f rand-data.csv -t 9000 9000  >> $now.log
java inventory.HashApp -f rand-data.csv -t 10000 10000  >> $now.log

